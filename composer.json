{
  "name": "provider/patreon",
  "description": "Patreon data provider for Porter.",
  "authors": [
    {
      "name": "Bilge",
      "email": "bilge@scriptfusion.com"
    }
  ],
  "minimum-stability": "beta",
  "prefer-stable": true,
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "connectors/http": "^6",
    "scriptfusion/porter": "^7"
  },
  "require-dev": {
    "amphp/http-client": "v5.0.0-beta.7",
    "amphp/socket": "v2.0.0-beta.9",
    "mockery/mockery": "^1.3.3",
    "phpunit/phpunit": "^9.5.23",
    "scriptfusion/mapper": "^2.4",
    "transformers/mapping-transformer": "^3"
  },
  "autoload": {
    "psr-4": {
      "ScriptFUSION\\Porter\\Provider\\Patreon\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ScriptFUSIONTest\\Porter\\Provider\\Patreon\\": "test"
    }
  },
  "scripts": {
    "test": "phpunit -c test"
  },
  "config": {
    "sort-packages": true
  }
}
